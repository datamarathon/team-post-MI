121      data WORK.FIXEDDATES    ;
122      %let _EFIERR_ = 0; /* set the ERROR detection macro variable */
123      infile 'C:\Users\ch151634\Dropbox\fixeddates.csv' delimiter = ';' MISSOVER DSD
123! lrecl=32767 firstobs=2 ;
124         informat VAR1 best32. ;
125         informat SUBJECT_ID comma32. ;
126         informat HADM_ID comma32. ;
127         informat GENDER $1. ;
128         informat DOB DATE9. ;
129         informat DOD $30. ;
130         informat EXPIRE_FLG $1. ;
131         informat HOSPITAL_ADMIT_DT DATE8. ;
132         informat HOSPITAL_DISCH_DT DATE9. ;
133         informat HOSPITAL_EXPIRE_FLG $1. ;
134         informat first_drug1 $4. ;
135         format VAR1 best12. ;
136         format SUBJECT_ID comma12. ;
137         format HADM_ID comma12. ;
138         format GENDER $1. ;
139         format DOB DATE9. ;
140         format DOD $30. ;
141         format EXPIRE_FLG $1. ;
142         format HOSPITAL_ADMIT_DT DATE8. ;
143         format HOSPITAL_DISCH_DT DATE9. ;
144         format HOSPITAL_EXPIRE_FLG $1. ;
145         format first_drug1 $4. ;
146      input
147                  VAR1
148                  SUBJECT_ID
149                  HADM_ID
150                  GENDER $
151                  DOB
152                  DOD $
153                  EXPIRE_FLG $
154                  HOSPITAL_ADMIT_DT
155                  HOSPITAL_DISCH_DT
156                  HOSPITAL_EXPIRE_FLG $
157                  first_drug1 $
158      ;
159      if _ERROR_ then call symputx('_EFIERR_',1);  /* set ERROR detection macro
159! variable */
160      run;
